<Window x:Class="CarmenUI.Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarmenUI"
        mc:Ignorable="d"
        Top="{local:SettingBinding Top}"
        Left="{local:SettingBinding Left}"
        Height="{local:SettingBinding Height}"
        Width="{local:SettingBinding Width}"
        WindowState="{local:SettingBinding WindowState}"
        MinHeight="600" MinWidth="800"
        Title="CARMEN">
    <Frame x:Name="MainFrame" Source="/Pages/MainMenu.xaml" NavigationUIVisibility="Hidden" Navigated="MainFrame_Navigated"/>
</Window>
